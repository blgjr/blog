<h1>Список постов</h1>
 
<%= link_to 'Новый пост', new_post_path %>

<table>
	<tr>
		<th>Заголовок</th>
		<th>Текст</th>
	</tr>

	<% @posts.each do |post| %>
		<tr>
			<td><%= post.title %></td>
			<td><%= post.text %></td>
			<td><%= link_to 'Показать', post_path(post) %></td>
		</tr>
	<% end %>
</table>